    <meta property="og:description" content="{{desc}}">
    <meta property="og:url" content="http://www.citychiropractor.com.au/">
    {{!-- //Meta --}}

    {{!-- Resource Hints --}}
    <link rel="preload" href="/fonts/source-sans-pro/sourcesanspro-regular.woff2" as="font" type="font/woff2">
    {{!-- //Resource Hints --}}

    {{!-- Favicons --}}
    <link rel="manifest" href="/manifest.json">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-TileImage" content="/mstile-144x144.png">
